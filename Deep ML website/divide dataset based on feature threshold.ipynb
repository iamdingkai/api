   "source": [
    "import numpy as np\n",
    "\n",
    "def divide_on_feature(X, feature_i, threshold):\n",
    "\t\n",
    "    X1 = [x for x in X if x[feature_i] >= threshold]\n",
    "    X2 = [x for x in X if x[feature_i] < threshold]\n",
    "\n",
    "    return [X1, X2]"
   ]
